{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/create-payment-intent",
        "dest": "/api/create-payment-intent.js"
      },
      {
        "src": "/api/payment-sheet",
        "dest": "/api/payment-sheet.js"
      }
    ],
    "functions": {
      "api/**/*.js": {
        "maxDuration": 10
      }
    }
  }